<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center">

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="933dp"
        android:layout_height="363dp"
        android:src="@mipmap/img_pop_factory_reset"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/dialog_factory2_card2_yes"
        android:layout_width="356dp"
        android:layout_height="70dp"
        android:scaleType="fitXY"
        android:src="@drawable/btn_factory_2_yes"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintLeft_toLeftOf="@+id/imageView6"
        app:layout_constraintRight_toRightOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.85" />


    <ImageView
        android:id="@+id/dialog_factory2_card2_no"
        android:layout_width="356dp"
        android:layout_height="70dp"
        android:scaleType="fitXY"
        android:src="@drawable/btn_factory_2_no"
        app:layout_constraintBottom_toBottomOf="@+id/imageView6"
        app:layout_constraintHorizontal_bias="0.9"
        app:layout_constraintLeft_toLeftOf="@+id/imageView6"
        app:layout_constraintRight_toRightOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        app:layout_constraintVertical_bias="0.85" />

</android.support.constraint.ConstraintLayout>
