<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/home_app_mode_baidu"
        android:layout_width="240dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/home_app_mode_weibo"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/home_app_mode_weibo"
        android:layout_width="240dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintLeft_toRightOf="@+id/home_app_mode_baidu"
        app:layout_constraintRight_toLeftOf="@+id/home_app_mode_i71"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/home_app_mode_i71"
        android:layout_width="240dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintLeft_toRightOf="@+id/home_app_mode_weibo"
        app:layout_constraintRight_toLeftOf="@+id/home_app_mode_avin"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/home_app_mode_avin"
        android:layout_width="240dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintLeft_toRightOf="@+id/home_app_mode_i71"
        app:layout_constraintRight_toLeftOf="@+id/home_app_mode_mp3"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/home_app_mode_mp3"
        android:layout_width="240dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toTopOf="@+id/workout_mode_quick_start"
        app:layout_constraintLeft_toRightOf="@+id/home_app_mode_avin"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/workout_mode_quick_start"
        android:layout_width="480dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/home_app_mode_i71" />


</android.support.constraint.ConstraintLayout>
